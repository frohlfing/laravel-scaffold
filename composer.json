{
    "name": "frohlfing/laravel-scaffold",
    "description": "Scaffold generator for Laravel 5.7",
    "keywords": ["laravel", "package", "development"],
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Rohlfing",
            "email": "mail@frank-rohlfing.de"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*",
        "frohlfing/base": "1.57.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "FRohlfing\\Scaffold\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.57.x-dev"
        },
        "laravel": {
            "providers": [
                "FRohlfing\\Scaffold\\ScaffoldServiceProvider"
            ]
        }
    }
}
